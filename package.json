  "dependencies": {
    "got": "^9.6.0"
  },
  "devDependencies": {
    "ava": "^2.3.0",
    "c8": "^5.0.1",
    "coveralls": "^3.0.6",
    "standard": "^14.0.0",
    "standard-version": "^7.0.0"
  }
